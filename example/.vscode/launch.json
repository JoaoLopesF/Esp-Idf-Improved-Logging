{
	"version": "0.1.0",
	"configurations": [
	
		{
			"name": "GDB/OpenOCD",
			"type": "gdb",
			"request": "attach",
			"printCalls": true,
			"executable": "${workspaceRoot}/build/relax_mind.elf",
			"target": "localhost:3333",
			"remote": true,
			"cwd": "${workspaceRoot}",
			"autorun": [
				"mon reset halt",
				"flushregs",
				"thb app_main",
				//"c",
			],
			"gdbpath": "/Improvelife/Desenv/Espressif/xtensa-esp32-elf/bin/xtensa-esp32-elf-gdb"
		}

	]
}
